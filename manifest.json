{
  "manifest_version": 2,
  "name": "LZipped Local Storage",
  "version": "0.2.1",
  "description": "LZipped Local Storage",
  "icons": {
    "16": "lzip16.png",
    "48": "lzip48.png",
    "128": "lzip128.png"
  },
  "devtools_page": "devtools.html",
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "background",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "web_accessible_resources": [
    "getstorage.js",
    "background.js"
  ]
}